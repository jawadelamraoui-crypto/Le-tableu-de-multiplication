#include <stdio.h>

int main() {
    int arr[100];
    for(int i = 0; i < 100; i++) {
        arr[i] = i + 1;
    }
    for(int i = 1; i < 100; i++) {
        if (arr[i] != 0) {
            for(int j = i + 1; j < 100; j++) {
                if(arr[j] != 0 && arr[j] % arr[i] == 0) {
                    arr[j] = 0;
                }
            }
        }
    }
    printf("Nombres restants aprÃ¨s suppression des multiples:\n");
    for(int i = 0; i < 100; i++) {
        if(arr[i] != 0)
            printf("%d ", arr[i]);
    }
    printf("\n");
    return 0;
}
