#include <stdio.h>

int main() {
    int N;
    printf("Entrez un nombre : ");
    scanf("%d", &N);

    for (int n = 2; n < N; n++) {
        int est_premier = 1;
        for (int i = 2; i < n; i++) {
            if (n % i == 0) {
                est_premier = 0;
                break;
            }
        }
        if (est_premier) {
            printf("%d ", n);
        }
    }

    return 0;
}
